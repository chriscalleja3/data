{
  "resources": [
    {
      "path": "data.csv",
      "name": "data",
      "profile": "tabular-data-resource",
      "scheme": "file",
      "format": "csv",
      "hashing": "md5",
      "encoding": "utf-8",
      "schema": {
        "fields": [
          {
            "name": "Year",
            "type": "integer"
          },
          {
            "name": "Sex",
            "type": "string",
            "constraints": {
              "enum": [
                "Male",
                "Female",
                ""
              ]
            }
          },
          {
            "name": "Age",
            "type": "string",
            "constraints": {
              "enum": [
                "<15Y",
                "15-64",
                "65+",
                ""
              ]
            }
          },
          {
            "name": "Location",
            "type": "string",
            "constraints": {
              "enum": [
                "Urban",
                ""
              ]
            }
          },
          {
            "name": "Value",
            "type": "number"
          }
        ]
      }
    }
  ],
  "profile": "data-package",
  "name": "1-1-1",
  "title": "Proportion of the population living below the international poverty line by sex, age, employment status and geographic location (urban/rural)"
}